<div class="wrapper">
  <div class="container">
    <h1>Upload File .txt</h1>
    <hr>
    <div class="row">
        <%= form_tag(upload_path, method: :post, multipart: true) do %>
          <div class="col-md-4"> 
              <div class="form-group form-inline ">
                <%= label_tag :file %>         
                <%= file_field_tag :file, class: "form-control" %>
                </div>
          </div>
          <div class="col-md-2">
            <%= submit_tag 'Upload', class: "btn btn-primary btn-sm btn-block" %>
          </div>
        <% end %>
    </div>
  </div>
</div>